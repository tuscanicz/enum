{
    "name": "tuscanicz/enum",
    "description": "PHP implementation of Enum with strict comparisons",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "enum"
    ],
    "authors": [
        {
            "name": "Karel Štefan",
            "email": "dev@karelstefan.cz"
        },
        {
            "name": "Petr Bechyně",
            "email": "mail@petrbechyne.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Enum": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "replace": {
        "kstefan/enum": "self.version"
    },
    "conflict": {
        "kstefan/enum": "<=1.0"
    }
}
